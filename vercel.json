{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/claude/v1/messages",
      "dest": "/api/claude.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "framework": null
}
